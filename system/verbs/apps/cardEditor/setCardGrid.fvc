on setCardGrid (gridOn, gridVisible, gridUnits) {
	local (atts);
	new (tableType, @atts);
	atts.grid = boolean (gridOn);
	atts.grvs = boolean (not gridVisible);
	atts.grun = number (gridUnits);
	return (cardEditor.setCardAttributes (@atts))}


