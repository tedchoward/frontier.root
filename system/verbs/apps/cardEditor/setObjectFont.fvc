on setObjectFont (name, fontName, fontSize, fontStyle) {
	new (tableType, @atts);
	atts.font = string (fontName);
	atts.size = number (fontSize);
	atts.styl = number (fontStyle);
	return (cardEditor.setObjectAttributes (name, @atts))}


