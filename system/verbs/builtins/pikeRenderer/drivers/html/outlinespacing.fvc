FrontierVcsFile:3:scpt:system.verbs.builtins.pikeRenderer.drivers.html.outlinespacing

on outlinespacing (spacingstyle, adrprestring, adrpoststring) {
	case spacingstyle {
		"1" {
			adrpoststring^ = adrpoststring^ + "<br>"};
		"1.5" {
			adrpoststring^ = adrpoststring^ + "<br><br>"};
		"2" {
			adrpoststring^ = adrpoststring^ + "<br><br><br>"}}}


