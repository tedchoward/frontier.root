FrontierVcsFile:3:scpt:system.verbs.builtins.mainResponder.getDateFileName

on getDateFileName (theDate) {
	local (day, month, year, hour, minute, second);
	date.get (theDate, @day, @month, @year, @hour, @minute, @second);
	return (year + "-" + string.padwithzeros (month, 2) + "-" + string.padwithzeros (day, 2))}


