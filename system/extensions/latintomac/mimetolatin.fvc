FrontierVcsFile:3:scpt:system.extensions.latintomac.mimetolatin

on mimetolatin (mimeText) {
	if frontier.isPowerPC () {
		return (appleEvent (@latintomac.codePPC, 'cvtx', 'rpmc', '----', string (mimeText), 'cvmc', boolean (false)))}
	else {
		return (appleEvent (@latintomac.code68K, 'cvtx', 'rpmc', '----', string (mimeText), 'cvmc', boolean (false)))}}


